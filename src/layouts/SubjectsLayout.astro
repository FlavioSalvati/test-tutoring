---
// src/pages/SubjectsLayout-admissions.astro
import Astro from 'astro'; // Ensure Astro is imported to access props
const { contentData, faqData} = Astro.props;
import MainLayout from '../layouts/MainLayout.astro';
import SubjectEnd from '../components/SubjectEnd.astro';
import OurProcess from '../components/OurProcess.astro';
import FaqList from '../components/FaqList.astro';
import SubjectWindow from '../components/SubjectWindow.astro';

---


<MainLayout >

<SubjectWindow content={contentData}/>
<OurProcess />
<FaqList questionsAndAnswers={faqData} />

<SubjectEnd
        imageSrc={contentData[0]?.endImageSrc}
        imageAlt = {contentData[0]?.endImageAlt}
        stat={contentData[0]?.endStat}
        content={contentData[0]?.endContent}
    />

</MainLayout>
